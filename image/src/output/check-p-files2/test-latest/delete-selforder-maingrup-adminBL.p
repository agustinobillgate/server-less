DEFINE INPUT PARAMETER maingrp-number1 AS INTEGER.
DEFINE OUTPUT PARAMETER int-flag AS INTEGER INITIAL 0.
DEFINE OUTPUT PARAMETER success-flag AS LOGICAL INITIAL NO.

FIND FIRST queasy WHERE queasy.KEY EQ 228
    AND queasy.number1 EQ maingrp-number1 EXCLUSIVE-LOCK NO-ERROR.
IF AVAILABLE queasy THEN
DO:
    DELETE queasy.
    RELEASE queasy.
    ASSIGN success-flag = YES.
END.
