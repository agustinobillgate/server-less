DEF OUTPUT PARAMETER ci-date     AS DATE    NO-UNDO.  
DEF OUTPUT PARAMETER vipnr1      AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER vipnr2      AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER vipnr3      AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER vipnr4      AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER vipnr5      AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER vipnr6      AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER vipnr7      AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER vipnr8      AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER vipnr9      AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER bfast-artnr AS INTEGER NO-UNDO.  
DEF OUTPUT PARAMETER bfast-dept  AS INTEGER NO-UNDO.  
  
RUN htpdate.p (87,  OUTPUT ci-date).  
RUN htpint.p  (125, OUTPUT bfast-artnr).  
RUN htpint.p  (126, OUTPUT bfast-dept).  
  
RUN get-vipnrbl.p (OUTPUT vipnr1, OUTPUT vipnr2, OUTPUT vipnr3, OUTPUT vipnr4,  
    OUTPUT vipnr5, OUTPUT vipnr6, OUTPUT vipnr7, OUTPUT vipnr8,  
    OUTPUT vipnr9).  
  
