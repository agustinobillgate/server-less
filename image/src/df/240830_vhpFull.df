ADD TABLE "reservation"
  AREA "Schema Area"
  DESCRIPTION "Reservation Data File"
  VALEXP "not can-find(first res-line of reservation)"
  VALMSG "Cannot delete a reservation before res-lines"
  DUMP-NAME "reservat"
  TABLE-TRIGGER "CREATE" NO-OVERRIDE PROCEDURE "tcreserv.p" CRC "?" 
  TABLE-TRIGGER "DELETE" NO-OVERRIDE PROCEDURE "tdreserv.p" CRC "?" 
  TABLE-TRIGGER "WRITE" NO-OVERRIDE PROCEDURE "twreserv.p" CRC "?" 

